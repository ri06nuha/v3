            <ambient>1.0 0.4 0.1 1</ambient>
            <diffuse>1.0 0.4 0.1 1</diffuse>
            <emissive>1.0 0.5 0.0 1</emissive>
          </material>
        </visual>
      </link>
    </model>

    <light name="fire_light" type="point">
      <pose>5 0 2 0 0 0</pose> <diffuse>1.0 0.6 0.1 1</diffuse>
