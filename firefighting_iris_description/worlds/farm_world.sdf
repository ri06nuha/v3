          <geometry><box><size>0.47 0.47 0.23</size></box></geometry>
          <surface>
            <contact>
              <ode><max_vel>100.0</max_vel><min_depth>0.001</min_depth></ode>
            </contact>
            <friction>
              <ode><mu>100000.0</mu><mu2>100000.0</mu2></ode>
            </friction>
