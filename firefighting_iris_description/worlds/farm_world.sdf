    <model name="realistic_fire">
      <pose>5 0 0.1 0 0 0</pose>
      <static>true</static>
      <link name="fire_core">
        <visual name="core">
          <geometry>
            <cylinder><radius>0.8</radius><length>1.5</length></cylinder>
          </geometry>
          <material>
            <ambient>1.0 0.2 0.0 1</ambient>
            <diffuse>1.0 0.2 0.0 1</diffuse>
